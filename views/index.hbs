                                        <div id="answer-1884799" class="answer ice-ice-baby" data-answerid="1884799" itemscope="" itemtype="http://schema.org/Answer">
                                            <div class="post-layout">
                                                <div class="votecell post-layout--left">


                                                    <div class="vote">
                                                        <input type="hidden" name="_id_" value="1884799">
                                                        <a class="vote-up-off" title="This answer is useful">up vote</a>
                                                        <span itemprop="upvoteCount" class="vote-count-post ">86</span>
                                                        <a class="vote-down-off" title="This answer is not useful">down vote</a>




                                                    </div>

