                                        <div id="answer-2619353" class="answer ice-ice-baby" data-answerid="2619353" itemscope="" itemtype="http://schema.org/Answer">
                                            <div class="post-layout">
                                                <div class="votecell post-layout--left">


                                                    <div class="vote">
                                                        <input type="hidden" name="_id_" value="2619353">
                                                        <a class="vote-up-off" title="This answer is useful">up vote</a>
                                                        <span itemprop="upvoteCount" class="vote-count-post ">13</span>
